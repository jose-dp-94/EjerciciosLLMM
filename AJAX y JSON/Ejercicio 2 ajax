<h2> El objeto XMLHttpRequest </h2>

<button type = "button" onclick = "loadDoc ()"> colección de CD </button>
<br> <br>
<p id = "demo"> </p>

<script>
función loadDoc () {
  var xhttp = nuevo XMLHttpRequest ();
  xhttp.onreadystatechange = function () {
    if (this.readyState == 4 && this.status == 200) {
      myFunction (esto);
    }
  };
  xhttp.open ("GET", "localidad_21002.xml", verdadero);
  xhttp.send ();
}
function myFunction (xml) {
  var i;
  var xmlDoc = xml.responseXML;
  var p = "";
  var x = xmlDoc.getElementsByTagName ("temperatura");
    p + = "";
    x [0] .getElementsByTagName ("maxima") [0] .childNodes [0] .nodeValue +
    "<br>" +
    x [0] .getElementsByTagName ("mínimos") [0] .childNodes [0] .nodeValue +
    "<br>";
  document.getElementById ("demo"). innerHTML = p;
}
</script>

